{
  "name": "clubfinder",
  "version": "1.0.0",
  "description": "An app to search club information",
  "main": "app.js",
  "scripts": {
    "start": "npx http-server ./dist",
    "lint": "npx eslint . -c config/.eslintrc.js --ext .js",
    "pretty": "npx prettier --check .",
    "format": "npx prettier --write .",
    "fix": "npx eslint . -c config/.eslintrc.js --ext .js --fix",
    "repair": "npm run format && npm run fix",
    "start-dev": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manabil/FrontEndFundamental.git#main"
  },
  "keywords": [
    "club",
    "football",
    "webpack",
    "bundler",
    "component"
  ],
  "author": "manabil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manabil/FrontEndFundamental/issues"
  },
  "homepage": "https://github.com/manabil/FrontEndFundamental/tree/main#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.27.0",
    "eslint-config-google": "^0.14.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "http-server": "^14.1.1",
    "regenerator-runtime": "^0.13.11"
  }
